<table class="table table-striped table-bordered">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>admin</th>
    <th>mod</th>
    <th></th>
    <th></th>
  </tr>
  <% @users.each do |user| %>
    <% if user.mod_id == nil %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>

        <% if user.admin? %>
          <td>trueeeeee</td>
        <% else %>
          <td>falseeeee</td>
        <% end %>
        <td><%= user.mod_id %></td>

        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
    <% end %>
  <% end %>
</table>